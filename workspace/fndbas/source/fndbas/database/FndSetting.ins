--
--  New_Parameter = Fnd_Setting_API.New_Parameter:
--     Create the parameter if it does not already exist. In this call
--     the default value is to be set. If parameter already exist, everything
--     except value_ and dynamic is updated.
--
--  Modify_Parameter = Fnd_Setting_API.Modify_Parameter:
--     Update an already existing parameter. Seldom used, in most cases no
--     setting of the default value is to be made, since the value may have been
--     changed by an administrator and this script should not change
--     such a value during an upgrade. Only the attributes that has been
--     changed since the parameter was created need to be set.
--
--  Remove_Parameter = Fnd_Setting_API.Remove_Parameter:
--     Remove obsolete parameters.
--
--  Fnd_Setting_API.Get_Parameter:
--     Fetch a parameter with all its attributes. Use this when some
--     evaluation of existing attributes has to be made when updating
--     a parameter.
--
--  Value types:
--    value_type_fixed_string_  = FIXED STRING  : (Default) Datatype string and only the values specified in the domain are allowed.
--    value_type_fixed_number_  = FIXED NUMBER  : Datatype number and only the values specified in the domain are allowed.
--    value_type_fixed_date_    = FIXED DATE    : Date format as a string and only the values specified in the domain are allowed.
--    value_type_custom_string_ = CUSTOM STRING : Datatype string and custom values are allowed.
--    value_type_custom_number_ = CUSTOM NUMBER : Datatype number and custom values are allowed.
--    value_type_custom_date_   = CUSTOM DATE   : Date format as a string and custom values are allowed.
-----------------------------------------------------------------------------

PROMPT Inserting Foundation1 Parameters

SET SERVEROUTPUT ON

DECLARE
   --
   -- Cursor declaration
   --
   CURSOR get_encoding IS
      SELECT value
      FROM  nls_database_parameters
      WHERE parameter = 'NLS_CHARACTERSET';
   --
   CURSOR get_user_ts IS
      SELECT Nvl(default_tablespace, 'USERS') default_tablespace,
             Nvl(temporary_tablespace, UPPER('&TEMP_TABLESPACE')) temporary_tablespace
      FROM user_users u;
   --
   CURSOR get_default_ts IS
      SELECT tablespace_name
      FROM  dba_tablespaces
      WHERE tablespace_name = 'USERS';
   --
   CURSOR get_temp_ts IS
      SELECT tablespace_name
      FROM  dba_tablespaces
      WHERE tablespace_name = UPPER('&TEMP_TABLESPACE');
   --
   -- Variable declaration
   --
   parameter_record_ fnd_setting_tab%ROWTYPE;
   value_            fnd_setting_tab.value%TYPE;
   dummy_            VARCHAR2(30);
