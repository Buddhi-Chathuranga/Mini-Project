   PROCEDURE Remove_Parameter(
      parameter_ IN VARCHAR2)
   IS
   BEGIN
      Fnd_Setting_API.Remove_Parameter(parameter_);
   END Remove_Parameter;
   
   PROCEDURE New_Parameter(
      parameter_        IN VARCHAR2,
      description_      IN VARCHAR2,
      dynamic_          IN VARCHAR2,
      value_            IN VARCHAR2,
      domain_           IN VARCHAR2,
      domain_desc_      IN VARCHAR2,
      category_         IN VARCHAR2,
      value_type_       IN VARCHAR2 DEFAULT 'FIXED STRING',
      client_read_only_ IN VARCHAR2 DEFAULT 'FALSE')
   IS
   BEGIN
      Fnd_Setting_API.New_Parameter(parameter_, description_, dynamic_, value_, domain_, domain_desc_, category_, value_type_, 'FNDBAS', client_read_only_);
   END New_Parameter;
  
   PROCEDURE Modify_Parameter(
      parameter_   IN VARCHAR2,
      description_ IN VARCHAR2,
      dynamic_     IN VARCHAR2,
      value_       IN VARCHAR2,
      domain_      IN VARCHAR2,
      domain_desc_ IN VARCHAR2,
      category_    IN VARCHAR2,
      value_type_  IN VARCHAR2 DEFAULT 'FIXED STRING')
   IS
   BEGIN
      Fnd_Setting_API.Modify_Parameter(parameter_, description_, dynamic_, value_, domain_, domain_desc_, category_, value_type_, 'FNDBAS');
   END Modify_Parameter;   
  
   PROCEDURE Create_Or_Modify_List_Parameter(
      parameter_   IN VARCHAR2,
      description_ IN VARCHAR2,
      dynamic_     IN VARCHAR2,
      value_       IN VARCHAR2,
      domain_desc_ IN VARCHAR2,
      category_    IN VARCHAR2,
      value_type_  IN VARCHAR2 DEFAULT 'FIXED STRING')
   IS
      TYPE value_table IS TABLE OF VARCHAR2(200)  INDEX BY BINARY_INTEGER; 
      value_list_ value_table;
      old_value_  VARCHAR2(4000);
      index_      NUMBER;
      modified_   BOOLEAN := FALSE;
   BEGIN
      Fnd_Setting_API.Create_Or_Modify_List_Param(parameter_, description_, dynamic_, value_, value_ ||'^^', domain_desc_, category_, value_type_, 'FNDBAS');
   END Create_Or_Modify_List_Parameter;
