   </static-area>
   <page-body>
   </page-body>
   <page>
      <properties>
         <page-type>repeating</page-type>
      </properties>
      <reference>
         <properties>
            <id>repeat-page-head</id>
         </properties>
      </reference>
      <reference>
         <properties>
            <id>page-body</id>
         </properties>
      </reference>
