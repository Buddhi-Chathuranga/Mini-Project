<?xml version="1.0" encoding="UTF-8"?>

<report-layout>
   <properties>
      <combined>portrait</combined>
      <font-family>Arial</font-family>
      <font-size>12pt</font-size>
      <height>27.94cm</height>
      <initial-page-number>1</initial-page-number>
      <margin-bottom>1cm</margin-bottom>
      <margin-left>1.5cm</margin-left>
