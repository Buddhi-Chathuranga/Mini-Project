<SQLDataSource>
  <Author>rodhlk</Author>
  <Component>FNDBAS</Component>
  <DescriptiveText>List the InMemory Accelaration Packages that are enabled. </DescriptiveText>
  <LastModified>2015-07-15-10.43.55</LastModified>
  <Name>Enabled IMAPs</Name>
  <ID>6d8cd481-e9c7-4c03-8563-7bdc51b93f3f</ID>
  <Select>
    <DataColumn>
      <Column>PACKAGE_NAME</Column>
      <Name>Package Name</Name>
    </DataColumn>
    <DataColumn>
      <Column>ACTUAL_I_M_SIZE</Column>
      <Name>Actual InMemory Usage (MB)</Name>
      <DataType>Number</DataType>
    </DataColumn>
  </Select>
  <Where>ENABLED = 1</Where>
  <View>IN_MEMORY_PACKAGE</View>
  <OrderBy>ACTUAL_I_M_SIZE DESC</OrderBy>
</SQLDataSource>