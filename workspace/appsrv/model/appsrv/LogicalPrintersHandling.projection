------------------------------ ENTITY DETAILS -------------------------------
@Override
entity LogicalPrinter {
   attribute PrinterId Text {
      label = "Printer ID";
      editable = [ETag = null];
   }
   attribute Description Text {
      label = "Description";
   }
}

