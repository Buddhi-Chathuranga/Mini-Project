VIEW Inv_Part_Config_Project_Tm IS
   Prompt = 'Inventory Part Config Project'
   Contract.Flags = 'PMI-L'
   Contract.Datatype = 'STRING(5)/UPPERCASE'
   Contract.Prompt = 'Site'
   Part_No.Flags = 'PMI-L'
   Part_No.Datatype = 'STRING(25)/UPPERCASE'
   Part_No.Prompt = 'Part No'
   Part_No.Ref = 'InventoryPart(contract)/NOCHECK'
   Description.Flags = 'A-IUL'
   Description.Datatype = 'STRING(200)'
   Description.Prompt = 'Description'
   Configuration_Id.Flags = 'PMI-L'
   Configuration_Id.Datatype = 'STRING(50)'
   Configuration_Id.Prompt = 'Configuration ID'
   Configuration_Id.Ref = 'ConfigurationSpec(part_no)/NOCHECK'
   Project_Id.Flags = 'PMI-L'
   Project_Id.Datatype = 'STRING(50)'
   Project_Id.Prompt = 'Project ID'
SELECT contract,
       part_no,
       Inventory_Part_API.Get_Description(contract, part_no) description,       
       configuration_id,
       project_id,
       contract|| CHR(31) ||part_no|| CHR(31) ||configuration_id|| CHR(31) ||project_id   objid,              
       NULL  objversion
FROM inventory_part_config_tab ipc2, open_project_site ops
WHERE ipc2.contract = ops.site
AND   (EXISTS (SELECT 1 FROM user_allowed_site_pub WHERE site = ipc2.contract) OR
      EXISTS (SELECT 1 FROM user_allowed_supply_site_pub WHERE supply_site = contract))
UNION ALL
SELECT contract,
       part_no,
       Inventory_Part_API.Get_Description(contract, part_no) description,       
       configuration_id,
       '*' project_id,
       contract|| CHR(31) ||part_no|| CHR(31) ||configuration_id|| CHR(31) ||'*'   objid,              
       NULL  objversion       
FROM INVENTORY_PART_CONFIG_TAB
WHERE  (EXISTS (SELECT 1 FROM user_allowed_site_pub WHERE site = contract) OR
        EXISTS (SELECT 1 FROM user_allowed_supply_site_pub WHERE supply_site = contract ))
UNION ALL
SELECT contract,
       part_no,
       Inventory_Part_API.Get_Description(contract, part_no) description,       
       configuration_id,
       '#' project_id,
       contract|| CHR(31) ||part_no|| CHR(31) ||configuration_id|| CHR(31) ||'#'   objid,              
       NULL  objversion       
FROM  INVENTORY_PART_CONFIG_TAB
WHERE (EXISTS (SELECT 1 FROM user_allowed_site_pub WHERE site = contract) OR
      EXISTS (SELECT 1 FROM user_allowed_supply_site_pub WHERE supply_site = contract ))
AND   EXISTS (SELECT 1 FROM open_project_site WHERE  site = contract);

