       Warehouse_Bay_Bin_API.Get_Max_Humidity(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)       max_humidity,
       Warehouse_Bay_Bin_API.Get_Receipts_Blocked(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)   receipts_blocked,
       loc.rowkey                                              objkey
FROM   warehouse_bay_bin_tab loc, inventory_location_group_pub locgroup
WHERE  locgroup.inventory_location_type IN ('SHIPMENT')
AND    loc.location_group = locgroup.location_group
AND    loc.location_group = locgroup.location_group
AND    loc.receipts_blocked = 'FALSE'
AND    EXISTS (SELECT 1 FROM warehouse_bay_tier_tab tier WHERE receipts_blocked = 'FALSE'
                                                           AND loc.contract     = tier.contract
                                                           AND loc.warehouse_id = tier.warehouse_id
                                                           AND loc.bay_id       = tier.bay_id
                                                           AND loc.tier_id      = tier.tier_id)
AND    EXISTS (SELECT 1 FROM warehouse_bay_row_tab r WHERE receipts_blocked = 'FALSE'
                                                         AND loc.contract     = r.contract
                                                         AND loc.warehouse_id = r.warehouse_id
                                                         AND loc.bay_id       = r.bay_id
                                                         AND loc.row_id       = r.row_id)
AND    EXISTS (SELECT 1 FROM warehouse_bay_tab bay WHERE receipts_blocked = 'FALSE'
                                                     AND loc.contract     = bay.contract
                                                     AND loc.warehouse_id = bay.warehouse_id
                                                     AND loc.bay_id       = bay.bay_id)
AND    EXISTS (SELECT 1 FROM warehouse_tab whse WHERE receipts_blocked = 'FALSE'
                                                  AND loc.contract     = whse.contract
                                                  AND loc.warehouse_id = whse.warehouse_id)
AND    EXISTS (SELECT 1 FROM site_invent_info_tab site WHERE receipts_blocked = 'FALSE'
                                                         AND loc.contract     = site.contract);


