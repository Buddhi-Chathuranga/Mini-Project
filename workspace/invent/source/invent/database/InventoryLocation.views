-------------------- PUBLIC VIEW DEFINITIONS --------------------------------
@ServerOnlyAccess
VIEW Inventory_Location_Pub IS
   Prompt = 'Inventory Location'
SELECT contract                                        contract,
       location_no                                     location_no,
       location_group                                  location_group,
       warehouse_id                                    warehouse,
       bay_id                                          bay_no,
       row_id                                          row_no,
       tier_id                                         tier_no,
       bin_id                                          bin_no,
       warehouse_id                                    warehouse_id,
       bay_id                                          bay_id,
       row_id                                          row_id,
       tier_id                                         tier_id,
       bin_id                                          bin_id,
       description                                     location_name,
       location_sequence                               location_sequence,
       route_order                                     bin_route_order,
       (SELECT route_order
           FROM warehouse_tab
           WHERE contract     = bin.contract
             AND warehouse_id = bin.warehouse_id)      warehouse_route_order,
       (SELECT route_order
           FROM warehouse_bay_tab
           WHERE contract     = bin.contract
             AND warehouse_id = bin.warehouse_id
             AND bay_id       = bin.bay_id)            bay_route_order,
       (SELECT route_order
           FROM warehouse_bay_row_tab
           WHERE contract     = bin.contract
             AND warehouse_id = bin.warehouse_id
             AND bay_id       = bin.bay_id
             AND row_id       = bin.row_id)            row_route_order,
       (SELECT route_order
           FROM warehouse_bay_tier_tab
           WHERE contract     = bin.contract
             AND warehouse_id = bin.warehouse_id
             AND bay_id       = bin.bay_id
             AND tier_id      = bin.tier_id)           tier_route_order,
       rowkey                        objkey
FROM   warehouse_bay_bin_tab bin;


-------------------- PRIVATE VIEW DEFINITIONS -------------------------------

VIEW Inventory_Location5 IS
   Prompt = 'Inventory Location'
   Bay_No.Flags = 'A-I-L'
   Row_No.Flags = 'A-I-L'
   Tier_No.Flags = 'A-I-L'
   Bin_No.Flags = 'A-I-L'
   Location_Name.Datatype = 'STRING(50)'
   Location_Group.Datatype = 'STRING(5)'
   Bin_Width.Flags = 'A---L'
   Bin_Height.Flags = 'A---L'
   Bin_Depth.Flags = 'A---L'
   Bin_Carrying_Capacity.Flags = 'A---L'
   Min_Temperature.Flags = 'A---L'
   Max_Temperature.Flags = 'A---L'
   Min_Humidity.Flags = 'A---L'
   Max_Humidity.Flags = 'A---L'
SELECT loc.contract                                                                                                                  contract,
       loc.location_no                                                                                                               location_no,
       loc.warehouse_id                                                                                                              warehouse,
       loc.bay_id                                                                                                                    bay_no,
       loc.row_id                                                                                                                    row_no,
       loc.tier_id                                                                                                                   tier_no,
       loc.bin_id                                                                                                                    bin_no,
       loc.description                                                                                                               location_name,
       loc.location_group                                                                                                            location_group,
       Inventory_Location_Type_API.Decode(locgroup.inventory_location_type)                                                          location_type,
       Warehouse_Bay_Bin_API.Get_Width_Capacity(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)     bin_width,
       Warehouse_Bay_Bin_API.Get_Height_Capacity(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)    bin_height,
       Warehouse_Bay_Bin_API.Get_Dept_Capacity(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)      bin_depth,
       Company_Invent_Info_API.Get_Uom_For_Length(Site_API.Get_Company(loc.contract))                                          uom_for_length,
       Warehouse_Bay_Bin_API.Get_Carrying_Capacity(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)  bin_carrying_capacity,
       Company_Invent_Info_API.Get_Uom_For_Weight(Site_API.Get_Company(loc.contract))                                          uom_for_weight,
       Warehouse_Bay_Bin_API.Get_Min_Temperature(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)    min_temperature,
       Warehouse_Bay_Bin_API.Get_Max_Temperature(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)    max_temperature,
       Company_Invent_Info_API.Get_Uom_For_Temperature(Site_API.Get_Company(loc.contract))                                     uom_for_temperature,
       Warehouse_Bay_Bin_API.Get_Min_Humidity(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)       min_humidity,
       Warehouse_Bay_Bin_API.Get_Max_Humidity(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id)       max_humidity,
       loc.rowkey                                              objkey,
       loc.rowid                                               objid
FROM   warehouse_bay_bin_tab loc, inventory_location_group_pub locgroup, user_allowed_site_pub
WHERE  locgroup.inventory_location_type IN ('PICKING','F','MANUFACTURING')
 AND   loc.location_group = locgroup.location_group
 AND   Warehouse_Bay_Bin_API.Get_Receipts_Blocked_Db(loc.contract, loc.warehouse_id, loc.bay_id, loc.tier_id, loc.row_id, loc.bin_id) = 'FALSE'
 AND   contract = site;

