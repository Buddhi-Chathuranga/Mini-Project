   <NAME>TransportDeliveryNote</NAME>
   <COMPONENT>INVENT</COMPONENT>
   <LU_NAME>TransportDeliveryNoteRep</LU_NAME>
   <TOP_LEVEL_BLOCK>DECLARE
   company_          VARCHAR2(20);
   language_code_    VARCHAR2(6);
   archive_notes_    VARCHAR2(200);
   forwarder_addr_   Forwarder_Info_Address_API.Public_Rec;
   from_addr_        Company_Address_API.Public_Rec;
   supp_addr_        Supplier_Info_Address_API.Public_Rec;
   comp_addr_        Company_Address_API.Public_Rec;
   ship_name_        VARCHAR2(2000);
   ship_address1_    VARCHAR2(35);
   ship_address2_    VARCHAR2(35);
   ship_zip_code_    VARCHAR2(35);
   ship_city_        VARCHAR2(35);
   ship_state_       VARCHAR2(35);
   ship_county_      VARCHAR2(35);
   ship_country_     VARCHAR2(2000);
   transport_doc_series_      VARCHAR2(30);
   saft_pt_movement_type_     VARCHAR2(30); 

   CURSOR get_header IS
      SELECT *
      FROM transport_delivery_note
      WHERE delivery_note_id = :DeliveryNoteId;
   header_rec_   get_header%ROWTYPE;

BEGIN
   Transport_Delivery_Note_API.Exist(:DeliveryNoteId);
   
   OPEN get_header;
   FETCH get_header INTO header_rec_;
   CLOSE get_header;

   company_ := Site_API.Get_Company(header_rec_.contract);
   IF (Company_API.Get_Positioning_Type(company_) = &apos;LEFT&apos; )THEN
      :AddressPosition := 1;
   ELSE
      :AddressPosition := 0;
   END IF;

   -- Set report language
   language_code_ := Fnd_Session_API.Get_Language;
   Archive_API.Set_Language(language_code_);

   -- Set report notes displayed in the InfoServices archive
   archive_notes_ := Language_SYS.Translate_Constant(lu_name_, &apos;DELIVNOTENO: Delivery Note No&apos;);
   archive_notes_ := archive_notes_ || &apos;: &apos;|| :DeliveryNoteId || &apos;, &apos;;
   archive_notes_ := archive_notes_ || Language_SYS.Translate_Constant(lu_name_, &apos;ALTDELIVNOTENO: Alternative Delivery Note No&apos;);
   archive_notes_ := archive_notes_ || &apos;: &apos;|| header_rec_.alt_delivery_note_id;
   
   Archive_API.Set_Notes(archive_notes_);
   
   :AltDeliveryNoteId    := header_rec_.alt_delivery_note_id;
   :Contract             := header_rec_.contract;
   :ContractDesc         := Site_API.Get_Description(header_rec_.contract);
   :AuthorizeCode        := header_rec_.authorize_code;
   :AuthorizeName        := Order_Coordinator_API.Get_Name(header_rec_.authorize_code);
   :RecipientCompany     := header_rec_.recipient_company;
   :RecipientCompanyName := Company_API.Get_Name(header_rec_.recipient_company);
   :VendorNo             := header_rec_.vendor_no;
   :VendorName           := Supplier_Info_API.Get_Name(header_rec_.vendor_no);
   
   :ForwardAgentId     := header_rec_.forward_agent_id;
   IF (header_rec_.forward_agent_id IS NOT NULL) THEN
      :ForwardAgentName   := Forwarder_Info_API.Get_Name(header_rec_.forward_agent_id);
      :ForwardAddressNo   := Forwarder_Info_Address_API.Get_Id_By_Type(header_rec_.forward_agent_id, :ForwardAddressNo);
      :ForwardAddressName := :ForwardAgentName;
      forwarder_addr_ := Forwarder_Info_Address_API.Get(header_rec_.forward_agent_id, :ForwardAddressNo);
      :ForwardAddr1       := forwarder_addr_.address1;
      :ForwardAddr2       := forwarder_addr_.address2;
      :ForwardAddr3       := forwarder_addr_.zip_code;
      :ForwardAddr4       := forwarder_addr_.city;
      :ForwardAddr5       := forwarder_addr_.state;
      :ForwardAddr6       := forwarder_addr_.county;
      :ForwardAddr7       := Iso_Country_API.Get_Description(forwarder_addr_.country);
   END IF;
   
   
   :DeliveryTerms      := header_rec_.delivery_terms;
   :DeliveryTermsDesc  := Order_Delivery_Term_API.Get_Description(header_rec_.delivery_terms);
   :ShipViaCode        := header_rec_.ship_via_code;
   :ShipViaDesc        := Mpccom_Ship_Via_API.Get_Description(header_rec_.ship_via_code);
   :DeliveryReasonId   := header_rec_.delivery_reason_id;
   :DeliveryReasonDesc := Delivery_reason_API.Get_Description(company_, header_rec_.delivery_reason_id);
   :LabelNote          := header_rec_.label_text;
   
   :CreateDate    := header_rec_.create_date;
   :TransportDate := header_rec_.transport_date;
   
   :FromAddressNo   := header_rec_.delivery_address_no;
   :FromAddressName := :ContractDesc;
   from_addr_ := Company_Address_API.Get(company_, header_rec_.delivery_address_no);
   :FromAddr1       := from_addr_.address1;
   :FromAddr2       := from_addr_.address2;
   :FromAddr3       := from_addr_.zip_code;
   :FromAddr4       := from_addr_.city;
   :FromAddr5       := from_addr_.state;
   :FromAddr6       := from_addr_.county;
   :FromAddr7       := Iso_Country_API.Get_Description(from_addr_.country);
   
   :SingleOccurrenceAddress := header_rec_.single_occurrence_address_db;
   IF (header_rec_.single_occurrence_address_db = Fnd_Boolean_API.DB_FALSE) THEN
      :ShipAddressNo   := header_rec_.ship_address_no;
      IF (header_rec_.vendor_no IS NOT NULL) IS NULL THEN
         ship_name_ := Supplier_Info_API.Get_Name(header_rec_.vendor_no);
         supp_addr_ := Supplier_Info_Address_API.Get(header_rec_.vendor_no, header_rec_.ship_address_no);
      ELSE
         ship_name_ := Company_API.Get_Name(header_rec_.recipient_company);
         comp_addr_ := Company_Address_API.Get(header_rec_.recipient_company, header_rec_.ship_address_no);
      END IF;
      ship_address1_    := NVL(comp_addr_.address1, supp_addr_.address1);
      ship_address2_    := NVL(comp_addr_.address2, supp_addr_.address2);
      ship_zip_code_    := NVL(comp_addr_.zip_code, supp_addr_.zip_code);
      ship_city_        := NVL(comp_addr_.city, supp_addr_.city);
      ship_state_       := NVL(comp_addr_.state, supp_addr_.state);
      ship_county_      := NVL(comp_addr_.county, supp_addr_.county);
      ship_country_     := Iso_Country_API.Get_Description(NVL(comp_addr_.country, supp_addr_.country));
   END IF;
   :ShipAddressName := NVL(ship_name_, header_rec_.ship_address_name);
   :ShipAddr1    := NVL(ship_address1_, header_rec_.ship_address1);
   :ShipAddr2    := NVL(ship_address2_, header_rec_.ship_address2);
   :ShipAddr3    := NVL(ship_zip_code_, header_rec_.ship_zip_code);
   :ShipAddr4    := NVL(ship_city_, header_rec_.ship_city);
   :ShipAddr5    := NVL(ship_state_, header_rec_.ship_state);
   :ShipAddr6    := NVL(ship_county_, header_rec_.ship_county);
   :ShipAddr7    := NVL(ship_country_, Iso_Country_API.Get_Description(header_rec_.ship_country_code));
   
   IF header_rec_.objstate = &apos;Cancelled&apos; THEN
      :Status := header_rec_.state;
   END IF;   
   :GrossWeight := header_rec_.gross_weight;
   :NetWeight   := header_rec_.net_weight;
   :Volume      := header_rec_.volume;

   -- Retrieve the document phrase(s) if any
   -- The phrases are concatenated into one single string
   :DocumentPhrase := Mpccom_Phrase_Text_API.Get_All_Phrases(language_code_, &apos;3&apos;);
   $IF Component_Invoic_SYS.INSTALLED $THEN
      :PageFooter1 := translate(Company_Invoice_Info_API.Get_Footing_Line(company_, 1), &apos;^&apos;, chr(9));
      :PageFooter2 := translate(Company_Invoice_Info_API.Get_Footing_Line(company_, 2), &apos;^&apos;, chr(9));
      :PageFooter3 := translate(Company_Invoice_Info_API.Get_Footing_Line(company_, 3), &apos;^&apos;, chr(9));
      :PageFooter4 := translate(Company_Invoice_Info_API.Get_Footing_Line(company_, 4), &apos;^&apos;, chr(9));
   $END
END;</TOP_LEVEL_BLOCK>
   <BLOCKS>
      <BLOCK>
         <NAME>TransactionLine</NAME>
         <CURSOR_DEFINITION>SELECT transaction_id        transaction_id,
       transaction_type_db   transaction_type_db,
       NULL                  transaction_code,
       NULL                  part_no,
       NULL                  part_desc,
       NULL                  qty_delivered,
       NULL                  unit_meas,
       NULL                  lot_batch_no,
       NULL                  serial_no,
       NULL                  part_no_notes,
       NULL                  customs_stat_no,
       contract_             contract
FROM transport_deliv_note_line
WHERE delivery_note_id = :DeliveryNoteId</CURSOR_DEFINITION>
         <BLOCK_DEFINITION>DECLARE
   contract_   transport_delivery_note_tab.contract%TYPE;
   inventory_rec_  Inventory_Transaction_Hist_API.Public_Rec; 
   Part_rec_        Inventory_Part_API.Public_Rec;
   $IF Component_Mfgstd_SYS.INSTALLED $THEN
      operation_rec_   Operation_History_API.Public_Rec;
   $END
BEGIN
   IF :TransactionTypeDb = Transport_Transaction_Type_API.DB_INVENTORY THEN
      inventory_rec_ := Inventory_Transaction_Hist_API.Get(:TransactionId);
      Part_rec_      := Inventory_Part_API.Get(:Contract, :PartNo);
      :PartNo        := inventory_rec_.Part_No;
      :PartDesc      := Inventory_Part_API.Get_Description(:Contract, :PartNo);
      :UnitMeas      := Part_rec_.Unit_Meas;
      :QtyDelivered  := inventory_rec_.Quantity;
      :LotBatchNo    := inventory_rec_.Lot_Batch_No;
      :SerialNo      := inventory_rec_.Serial_No;
      :CustomsStatNo := Part_rec_.Customs_Stat_No;
      :PartNoNotes   := Document_Text_API.Get_All_Notes(Part_rec_.Note_Id, &apos;3&apos;);
   ELSE
      $IF Component_Shpord_SYS.INSTALLED AND Component_Mfgstd_SYS.INSTALLED $THEN
         operation_rec_ := Operation_History_API.Get(:TransactionId);
         :PartNo       := operation_rec_.part_no;
         :QtyDelivered :=  Shop_Order_Operation_API.Get_Outside_Qty_Shipped(operation_rec_.order_no, operation_rec_.release_no, operation_rec_.sequence_no, operation_rec_.operation_no);
      $ELSE
         NULL;
      $END
      $IF Component_Purch_SYS.INSTALLED $THEN
         :PartDesc    := Purchase_Part_API.Get_Description(:Contract, :PartNo);
         :UnitMeas    := Purchase_Part_API.Get_Default_Buy_Unit_Meas(:Contract, :PartNo);
         :PartNoNotes := Document_Text_API.Get_All_Notes(Purchase_Part_API.Get_Note_Id(:Contract, :PartNo), &apos;3&apos;);
      $END
      
   END IF;
END;</BLOCK_DEFINITION>
