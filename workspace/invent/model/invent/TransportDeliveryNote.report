      <BLOCK>
         <NAME>PageFooterRow</NAME>
         <CURSOR_DEFINITION>SELECT
   NULL            column1,   
   NULL            column2,   
   NULL            column3,   
   NULL            column4,   
   NULL            column5,   
   NULL            column6,   
   NULL            column7,
   page_footer_    page_footer
FROM dual</CURSOR_DEFINITION>
         <BLOCK_DEFINITION>DECLARE
   j_                 NUMBER := 1;
   temp_index1_       NUMBER;
   temp_index2_       NUMBER;
   pagefoot_length_   NUMBER;
   no_more_values_    BOOLEAN := FALSE;
   substr_length_     NUMBER;
BEGIN
   IF :PageFooter IS NOT NULL THEN
      pagefoot_length_ := LENGTH(:PageFooter);
      WHILE j_ &lt; 8  LOOP
         IF j_ = 1 THEN
            temp_index1_ := INSTR(:PageFooter, CHR(9),1,j_);
            temp_index2_ := INSTR(:PageFooter, CHR(9),1,j_+1);
         ELSE
            temp_index1_ := INSTR(:PageFooter, CHR(9),1,j_-1);
            temp_index2_ := INSTR(:PageFooter, CHR(9),1,j_);
         END IF;
         
         IF temp_index1_ = 0 THEN
            :Column1  := SUBSTR(Substr(:PageFooter, 1, pagefoot_length_), 1, 100);
            EXIT;
         ELSE
            IF temp_index2_ = 0 THEN
               substr_length_  := pagefoot_length_;
               no_more_values_ := TRUE;
            ELSE
               substr_length_ := temp_index2_-(temp_index1_+1);
            END IF;
            
            IF j_ = 1 THEN
               :Column1 := SUBSTR(SUBSTR(:PageFooter, 1, temp_index1_-1), 1, 100);
            ELSIF j_ = 2 THEN
               :Column2 := SUBSTR(SUBSTR(:PageFooter, temp_index1_+1, substr_length_), 1, 100);
            ELSIF j_ = 3 THEN
               :Column3 := SUBSTR(SUBSTR(:PageFooter, temp_index1_+1, substr_length_), 1, 100);
            ELSIF j_ = 4 THEN
               :Column4 := SUBSTR(SUBSTR(:PageFooter, temp_index1_+1, substr_length_), 1, 100);
            ELSIF j_ = 5 THEN
               :Column5 := SUBSTR(SUBSTR(:PageFooter, temp_index1_+1, substr_length_), 1, 100);
            ELSIF j_ = 6 THEN
               :Column6 := SUBSTR(SUBSTR(:PageFooter, temp_index1_+1, substr_length_), 1, 100);
            ELSIF j_ = 7 THEN
               :Column7 := SUBSTR(SUBSTR(:PageFooter, temp_index1_+1, substr_length_), 1, 100);
            END IF;
         END IF;
         j_ := j_ + 1;
         EXIT WHEN no_more_values_;
      END LOOP;
   END IF;
END;</BLOCK_DEFINITION>
         <PARAMETERS>
            <BLOCK_PARAMETER>
               <NAME>PageFooter</NAME>
               <DB_DATATYPE>VARCHAR2</DB_DATATYPE>
            </BLOCK_PARAMETER>
