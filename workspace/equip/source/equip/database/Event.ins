SET SERVEROUTPUT ON 

PROMPT Registering event EQUIP_OBJECT_WARRANTY for Equipment Object

DECLARE
BEGIN

  Event_SYS.Enable_Event( 'EquipmentObject', 'EQUIP_OBJECT_WARRANTY',
                          'Equipment object warranty will expire',
       'EVENT_DATETIME/DATETIME^USER_IDENTITY/STRING^USER_DESCRIPTION/STRING^'
    || 'USER_MAIL_ADDRESS/STRING^USER_MOBILE_PHONE/STRING^'
    || 'OBJECT_ID/STRING^OBJECT_DESCRIPTION/STRING^CONTRACT/STRING^'
    || 'WARRANTY_EXPIRES/DATE^COST_CENTER/STRING^SUPPLIER_CODE/STRING^'||'OBJKEY/STRING^');
END;
