<?xml version="1.0" encoding="UTF-8"?>
<FILTER xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:ifsworld-com:schemas:filter_filter">
   <NAME>SalesPartAssortmentFilter</NAME>
   <COMPONENT>ORDER</COMPONENT>
   <APPLIES>
      <APPLIED_TO>
         <SQL_WHERE_STMT>WHERE &amp;0.catalog_no IN ( SELECT part_no FROM MOBMGR_USER_SALES_PART )</SQL_WHERE_STMT>
         <ENTITY>SalesPart</ENTITY>
      </APPLIED_TO>
   </APPLIES>
</FILTER>
