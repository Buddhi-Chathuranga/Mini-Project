<?xml version="1.0" encoding="UTF-8"?>
<FILTER xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:ifsworld-com:schemas:filter_filter">
   <NAME>CanGetDeliveryNote</NAME>
   <COMPONENT>ORDER</COMPONENT>
   <APPLIES>
      <APPLIED_TO>
         <SQL_WHERE_STMT>WHERE order_no IN (SELECT order_no  FROM  customer_order_delivery_notes)
</SQL_WHERE_STMT>
         <ENTITY>CustomerOrder</ENTITY>
      </APPLIED_TO>
   </APPLIES>
</FILTER>
