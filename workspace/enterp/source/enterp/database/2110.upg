BEGIN
   IF (Seq_No_Already_Updated('ENTERP')) THEN
      Database_SYS.Remove_Table('PERSONAL_DATA_MAN_DET_ENTERP');
      RETURN;
   END IF;
   Load_V9_Pdm_Details;
   Update_Existing_Pdm_Det;
   Add_New_App10_Pdm_Det;
   COMMIT;
