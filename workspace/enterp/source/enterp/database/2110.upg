PROMPT Fill temporary personal_data_man_det_enterp table and convert app 10 pdm details
DECLARE
   msg_                      VARCHAR2(2000);
   FUNCTION Seq_No_Already_Updated (
      module_ IN VARCHAR2 ) RETURN BOOLEAN
   IS
      CURSOR check_updated IS
         SELECT count(*)
         FROM   personal_data_management_tab m, personal_data_man_det_tab d
         WHERE  d.pers_data_management_id = m.pers_data_management_id
         AND    m.system_defined = 'TRUE'
         AND    d.module = module_
         AND    d.seq_no < 0;
      dummy_   NUMBER;
   BEGIN
      OPEN  check_updated;
      FETCH check_updated INTO dummy_;
      CLOSE check_updated;
      IF (dummy_ > 0) THEN
        RETURN TRUE;
      END IF;
      RETURN FALSE;
   END Seq_No_Already_Updated;
   PROCEDURE Insert_V9_Pdm_Details (
      pers_data_management_id_  IN NUMBER,
      seq_no_                   IN NUMBER,
      cleanup_order_            IN NUMBER,
      storage_type_             IN VARCHAR2,
      data_subject_             IN VARCHAR2,
      logical_unit_id_          IN VARCHAR2,
      field_name_               IN VARCHAR2,
      field_value_              IN VARCHAR2,
      masked_value_             IN VARCHAR2,
      match_by_                 IN VARCHAR2,
      condition_                IN VARCHAR2,
      table_condition_          IN VARCHAR2,
      skip_anonymize_           IN VARCHAR2,
      mandatory_                IN VARCHAR2,
      reference_                IN VARCHAR2,
      exclude_from_cleanup_     IN VARCHAR2,
      report_order_             IN NUMBER,
      module_                   IN VARCHAR2,
      remarks_                  IN VARCHAR2 DEFAULT NULL )
   IS
      v9_rec_                   personal_data_man_det_enterp%ROWTYPE;
   BEGIN
      v9_rec_.pers_data_management_id := pers_data_management_id_;
      v9_rec_.seq_no                  := seq_no_;
      v9_rec_.cleanup_order           := cleanup_order_;
      v9_rec_.storage_type            := storage_type_;
      v9_rec_.data_subject            := data_subject_;
      v9_rec_.logical_unit_id         := logical_unit_id_;
      v9_rec_.field_name              := field_name_;
      v9_rec_.field_value             := field_value_;
      v9_rec_.masked_value            := masked_value_;
      v9_rec_.match_by                := match_by_;
      v9_rec_.mandatory               := mandatory_;
      v9_rec_.condition               := condition_;
      v9_rec_.table_condition         := table_condition_;
      v9_rec_.reference               := reference_;
      v9_rec_.skip_anonymize          := skip_anonymize_;
      v9_rec_.exclude_from_cleanup    := exclude_from_cleanup_;
      v9_rec_.report_order            := report_order_;
      v9_rec_.module                  := module_;
      v9_rec_.remarks                 := remarks_;
      v9_rec_.rowversion              := sysdate;
      INSERT INTO PERSONAL_DATA_MAN_DET_ENTERP VALUES v9_rec_;
   END Insert_V9_Pdm_Details;
