      New_Personal_Data_Man_Det___(33,-151,20,'FIELD','SUPPLIER','CommMethod','NAME',NULL,NULL,'IDENTITY','PARTY_TYPE_DB = ''SUPPLIER'' AND METHOD_ID_DB = ''WWW''','PARTY_TYPE = ''SUPPLIER'' AND METHOD_ID = ''WWW''','FALSE','FALSE','FALSE','FALSE',NULL,'ENTERP');
      New_Personal_Data_Man_Det___(33,-152,10,'LOGICAL_UNIT','SUPPLIER','CommMethod','NAME,DESCRIPTION,VALUE,VALID_FROM,VALID_TO',NULL,NULL,'IDENTITY','PARTY_TYPE_DB = ''SUPPLIER'' AND METHOD_ID_DB = ''WWW''','PARTY_TYPE = ''SUPPLIER'' AND METHOD_ID = ''WWW''','TRUE','FALSE','FALSE','FALSE',1,'ENTERP');
      New_Personal_Data_Man_Det___(33,-153,20,'FIELD','SUPPLIER','CommMethod','VALUE',NULL,NULL,'IDENTITY','PARTY_TYPE_DB = ''SUPPLIER'' AND METHOD_ID_DB = ''WWW''','PARTY_TYPE = ''SUPPLIER'' AND METHOD_ID = ''WWW''','FALSE','TRUE','FALSE','FALSE',NULL,'ENTERP');
      -- 4. fix data in temp_personal_data_man_det_tab
      stmt_ := 'SELECT org.storage_type org_storage_type, org.data_subject org_data_subject, org.application_area_id org_application_area_id,
                       org.field_name org_field_name, org.match_by org_match_by, org.mandatory org_mandatory, org.condition org_condition,
                       org.field_type org_field_type, org.field_data_length org_field_data_length, org.reference org_reference, org.seq_no org_seq_no,
                       org.skip_anonymize, org.report_order
                  FROM temp_personal_data_man_det_tab cpy,
                       personal_data_man_det_tab org
                 WHERE cpy.seq_no = org.seq_no
                   AND cpy.module = :module_
                   AND (   cpy.storage_type != org.storage_type
                        OR cpy.data_subject != org.data_subject
                        OR cpy.application_area_id != org.application_area_id
                        OR cpy.field_name != org.field_name
                        OR cpy.match_by != org.match_by
                        OR cpy.mandatory != org.mandatory
                        OR cpy.condition != org.condition
                        OR cpy.field_type != org.field_type
                        OR cpy.field_data_length != org.field_data_length
                        OR cpy.reference != org.reference
                        OR cpy.report_order != org.report_order)';
      OPEN pdm_to_fix_cursor FOR stmt_ USING 'ENTERP';
      LOOP
         FETCH pdm_to_fix_cursor INTO org_storage_type_, org_data_subject_, org_application_area_id_,
         org_field_name_, org_match_by_, org_mandatory_, org_condition_,
         org_field_type_, org_field_data_length_, org_reference_, org_seq_no_,
         org_skip_anonymize_, org_report_order_;
         EXIT WHEN pdm_to_fix_cursor%NOTFOUND;
         upd_stmt_ := 'UPDATE temp_personal_data_man_det_tab
                          SET storage_type = :org_storage_type_, data_subject = :org_data_subject_, application_area_id = :org_application_area_id,
                              field_name = :org_field_name_, match_by = :org_match_by_, mandatory = :org_mandatory_, condition = :org_condition_,
                              field_type = :org_field_type_, field_data_length = :org_field_data_length_, reference = :org_reference_,
                              skip_anonymize = :org_skip_anonymize_, report_order = :org_report_order_
                        WHERE seq_no = :org_seq_no_';
         EXECUTE IMMEDIATE upd_stmt_ USING org_storage_type_, org_data_subject_, org_application_area_id_,
         org_field_name_, org_match_by_, org_mandatory_, org_condition_,
         org_field_type_, org_field_data_length_, org_reference_, org_skip_anonymize_,org_report_order_,
         org_seq_no_;
      END LOOP;
      CLOSE pdm_to_fix_cursor;
      -- 5. delete duplicate entries from temp_personal_data_man_det_tab
      stmt_ := 'DELETE FROM temp_personal_data_man_det_tab tmp
                 WHERE (tmp.pers_data_management_id,tmp.seq_no) IN (SELECT b.pers_data_management_id,b.seq_no
                                                     FROM personal_data_man_det_tab A RIGHT OUTER JOIN temp_personal_data_man_det_tab B
                                                       ON a.pers_data_management_id = b.pers_data_management_id AND a.seq_no = b.seq_no
                                                    WHERE a.pers_data_management_id IS NULL AND a.seq_no IS NULL AND b.module = ''ENTERP'')';
      EXECUTE IMMEDIATE stmt_;
      -- 6. replace data in personal_data_man_det_tab with corrected data from temp_personal_data_man_det_tab
      DELETE FROM personal_data_man_det_tab;
      stmt_ := 'INSERT INTO personal_data_man_det_tab SELECT * FROM temp_personal_data_man_det_tab';
      EXECUTE IMMEDIATE stmt_;
      -- 7. remove temporary table temp_personal_data_man_det_tab
      Database_SYS.Remove_Table('TEMP_PERSONAL_DATA_MAN_DET_TAB', TRUE);
      -- 8.DONE!
      COMMIT;
   END IF;
END;
/

