   PROCEDURE Update_Existing_Pdm_Det
   IS
      stmt_  VARCHAR2(4000);
   BEGIN
      IF (Database_SYS.Column_Exist('PERSONAL_DATA_MAN_DET_TAB','PREV_SEQ_NO')) THEN
         stmt_ := 'DECLARE
                      CURSOR get_app9_seq IS
                         SELECT t.seq_no, t.module, t.pers_data_management_id, t.logical_unit_id, t.field_name, t.data_subject
                         FROM   personal_data_man_det_enterp t
                         WHERE  t.module = ''ENTERP'';
                   BEGIN
                      FOR a9 IN get_app9_seq LOOP
                         UPDATE personal_data_man_det_tab a10
                            SET a10.prev_seq_no = a10.seq_no,
                                a10.seq_no      = a9.seq_no
                          WHERE a10.pers_data_management_id = a9.pers_data_management_id
                          AND   a10.application_area_id     = a9.logical_unit_id
                          AND   ((a10.field_name            = a9.field_name) OR (a10.field_name is null and a9.field_name is null))
                          AND   a10.data_subject            = a9.data_subject;
                      END LOOP;
                   END';
         EXECUTE IMMEDIATE stmt_;
      END IF;
   END Update_Existing_Pdm_Det;
   PROCEDURE Update_New_App10_Det (
      pers_data_management_id_  IN NUMBER,
      seq_no_                   IN NUMBER,
      storage_type_             IN VARCHAR2,
      data_subject_             IN VARCHAR2,
      application_area_id_      IN VARCHAR2,
      field_name_               IN VARCHAR2 )
   IS
   BEGIN
      UPDATE personal_data_man_det_tab v10
         SET v10.seq_no = seq_no_
       WHERE v10.pers_data_management_id = pers_data_management_id_
         AND v10.storage_type            = storage_type_
         AND v10.data_subject            = data_subject_
         AND v10.application_area_id     = application_area_id_
         AND ((v10.field_name            = field_name_) OR (v10.field_name is null and field_name_ is null));
   END Update_New_App10_Det;
   PROCEDURE Add_New_App10_Pdm_Det
   IS
   BEGIN
      -- SEQ_NO = -154 reserved for MobileDeviceLocation
      Update_New_App10_Det(2,-155,'FIELD','CUSTOMER','CustomerInfoAddress','ADDRESS3');
