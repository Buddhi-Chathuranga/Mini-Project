   PROCEDURE New_Personal_Data_Man_Det___ (
      pers_data_management_id_  IN NUMBER,
      seq_no_                   IN NUMBER,
      cleanup_order_            IN NUMBER,
      storage_type_             IN VARCHAR2,
      data_subject_             IN VARCHAR2,
      application_area_id_      IN VARCHAR2,
      field_name_               IN VARCHAR2,
      field_value_              IN VARCHAR2,
      masked_value_             IN VARCHAR2,
      match_by_                 IN VARCHAR2,
      condition_                IN VARCHAR2,
      table_condition_          IN VARCHAR2,
      skip_anonymize_           IN VARCHAR2,
      mandatory_                IN VARCHAR2,
      reference_                IN VARCHAR2,
      exclude_from_cleanup_     IN VARCHAR2,
      report_order_             IN NUMBER,
      module_                   IN VARCHAR2,
      remarks_                  IN VARCHAR2 DEFAULT NULL )
   IS
      loc_reference_db_         personal_data_man_det_tab.reference%TYPE := reference_;
      loc_mandatory_db_         personal_data_man_det_tab.mandatory%TYPE := mandatory_;
      loc_field_type_db_        personal_data_man_det_tab.field_type%TYPE := NULL;
      loc_field_data_length_    personal_data_man_det_tab.field_data_length%TYPE := NULL;
      loc_skip_anonymize_       personal_data_man_det_tab.skip_anonymize%TYPE := NULL;
      pdm_basic_data_cursor     PdbBasicDataCursorType;
      loc_stmt_                 VARCHAR2(2000);
   BEGIN
      IF (storage_type_ = 'FIELD') THEN
         -- some fields are 'calculated' using Dictionary which is not availabe when .cdb is executed
         -- take data from backup table
         loc_stmt_ := 'SELECT reference, mandatory, skip_anonymize, field_type,field_data_length FROM personal_data_man_det_backup WHERE pers_data_management_id = :pdm_id AND seq_no = :seq';
         OPEN  pdm_basic_data_cursor FOR loc_stmt_ USING pers_data_management_id_, seq_no_;
         FETCH pdm_basic_data_cursor INTO loc_reference_db_, loc_mandatory_db_, loc_skip_anonymize_, loc_field_type_db_, loc_field_data_length_;
         IF pdm_basic_data_cursor%NOTFOUND THEN
            loc_stmt_ := 'SELECT reference, mandatory, skip_anonymize, field_type,field_data_length FROM personal_data_man_det_backup WHERE pers_data_management_id = :pdm_id AND storage_type = :str_type AND data_subject = :data_sub AND application_area_id = :lu_id AND nvl(field_name,'' '') = nvl(:fld_name,'' '') AND nvl(field_value,'' '') = nvl(:fld_value,'' '')';
            OPEN  pdm_basic_data_cursor FOR loc_stmt_ USING pers_data_management_id_, storage_type_, data_subject_, application_area_id_, field_name_, field_value_;
            FETCH pdm_basic_data_cursor INTO loc_reference_db_, loc_mandatory_db_, loc_skip_anonymize_, loc_field_type_db_, loc_field_data_length_;
            CLOSE pdm_basic_data_cursor;
         ELSE
            CLOSE pdm_basic_data_cursor;
         END IF;

         IF loc_skip_anonymize_ IS NULL THEN loc_skip_anonymize_ := skip_anonymize_;
         END IF;
      ELSE
         loc_skip_anonymize_ := skip_anonymize_;
      END IF;

      INSERT INTO personal_data_man_det_tab (pers_data_management_id, seq_no, cleanup_order,
                                             storage_type, data_subject, application_area_id, field_name, field_value, masked_value,
                                             match_by, mandatory, condition, field_type, field_data_length,
                                             reference, skip_anonymize, exclude_from_cleanup, report_order,
                                             module, table_condition, remarks,
                                             rowversion, rowkey)
      VALUES (pers_data_management_id_, seq_no_, cleanup_order_,
              storage_type_, data_subject_, application_area_id_, field_name_, field_value_, masked_value_,
              match_by_, loc_mandatory_db_, condition_, loc_field_type_db_, loc_field_data_length_,
              loc_reference_db_, loc_skip_anonymize_, exclude_from_cleanup_, report_order_,
              module_, table_condition_, remarks_,
              sysdate, sys_guid());
   END New_Personal_Data_Man_Det___;
