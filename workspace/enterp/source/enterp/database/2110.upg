EXCEPTION
   WHEN OTHERS THEN
      dbms_output.Enable;
      msg_ := SQLERRM;
      dbms_output.Put_Line(msg_);
      Database_SYS.Remove_Table('PERSONAL_DATA_MAN_DET_ENTERP');
END;
