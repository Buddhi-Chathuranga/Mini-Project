PROMPT Inserting Mpccpm_System_Event, Mpccom_Transaction_Code, Accounting_Event Default Data...

DECLARE
   trans_code_rec_ MPCCOM_TRANSACTION_CODE_TAB%ROWTYPE;
   acc_event_rec_  ACCOUNTING_EVENT_TAB%ROWTYPE;
BEGIN
   Mpccom_System_Event_API.Insert_Or_Update__('NISS', 'Issue from Inventory');

   trans_code_rec_.direction                   := '-';
   trans_code_rec_.order_type                  := '';
   trans_code_rec_.source_application          := 'MANUAL-ISSUES';
   trans_code_rec_.transaction_code            := 'NISS';
   trans_code_rec_.inventory_stat_direction    := 'ISSUE';
   trans_code_rec_.consignment_stock           := 'NOT CONSIGNMENT';
   trans_code_rec_.corresponding_transaction   := 'CO-NISS';
   trans_code_rec_.part_tracing                := 'USAGE-TRACING';
   trans_code_rec_.actual_cost_receipt         := 'NOT AFFECTED';
   trans_code_rec_.cost_source                 := 'OTHER';
   trans_code_rec_.notc                        := '';
   trans_code_rec_.intrastat_direction         := '';
   trans_code_rec_.customer_owned_stock        := 'CUSTOMER OWNED STOCK NOT ALLOWED';
   trans_code_rec_.supplier_loaned_stock       := 'SUPPLIER LOANED STOCK NOT ALLOWED';
   trans_code_rec_.transit_qty_direction       := 'UNAFFECTED QUANTITY';
   trans_code_rec_.trans_based_reval_group     := 'OTHER';
   trans_code_rec_.transaction_source          := 'INVENTORY';
   trans_code_rec_.company_owned_allowed       := 'TRUE';
   trans_code_rec_.receipt_issue_tracking      := 'TRUE';
   trans_code_rec_.company_rent_asset_allowed  := 'TRUE';
   trans_code_rec_.supplier_rented_allowed     := 'FALSE';
   trans_code_rec_.qty_at_customer_direction   := 'UNAFFECTED QUANTITY';
   trans_code_rec_.part_move_tax_direction     := '';
   trans_code_rec_.rowversion                  := sysdate;
   Mpccom_Transaction_Code_API.Insert_Or_Update__(trans_code_rec_);

   acc_event_rec_.event_code              := 'NISS';
   acc_event_rec_.authorize_id            := '*';
   acc_event_rec_.ms_addition_flag        := 'N';
   acc_event_rec_.material_addition_flag  := 'N';
   acc_event_rec_.oh1_burden_flag         := 'N';
   acc_event_rec_.oh2_burden_flag         := 'N';
   acc_event_rec_.online_flag             := 'Y';
   acc_event_rec_.delivery_overhead_flag  := 'N';
   acc_event_rec_.labor_overhead_flag     := 'N';
   acc_event_rec_.general_overhead_flag   := 'N';
   acc_event_rec_.consignment_event       := 'NOT CONSIGNMENT';
   acc_event_rec_.sales_overhead_flag     := 'FALSE';
   acc_event_rec_.rowversion              := sysdate;
   Accounting_Event_API.Insert_Or_Update__(acc_event_rec_);
END;
/
COMMIT;


