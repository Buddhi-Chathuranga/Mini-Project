UPDATE STATISTIC_PERIOD_TAB SET PERIOD_CLOSED = 'Y'
WHERE  trunc(SYSDATE) >= END_DATE
AND    PERIOD_CLOSED IS NULL;

--       **  WARNING  **  WARNING **  WARNING **  WARNING  **  WARNING  **
--  The string 'PURCHASE' that is INSERTed into the table OUTPUT_TYPE_TAB in the following
--  statement must NOT under any circumstances be changed in any way. Do not attempt to
--  translate into any other language. The value will not be visible in the user interface.Any changes
--  of the string value will lead to malfunction of purchase order lines without part numbers.
--       **  WARNING  **  WARNING **  WARNING **  WARNING  **  WARNING  **

INSERT INTO OUTPUT_TYPE_TAB (OUTPUT_TYPE, DESCRIPTION,ROWVERSION)
 VALUES ('PURCHASE', 'PURCHASE DOCUMENT',SYSDATE);


DECLARE
   start_date_              DATE; -- the start date for all units (JAN 1 of previous year)
   start_date_day_          DATE; -- date counter for inserting days
   start_date_week_         DATE; -- date counter for inserting weeks
   start_date_last_week_    DATE; -- date counter for inserting last week
   start_date_month_        DATE; -- date counter for inserting months
   start_date_quarter_      DATE; -- date counter for inserting quarters
   start_year_              NUMBER;
   plan_period_unit_def_no_ NUMBER;
   first_day_first_week_    VARCHAR2(20); -- day of the week of JAN 1 of current year
   last_day_last_week_      VARCHAR2(20); -- day of the week of DEC 31 of current year
   days_first_week_         VARCHAR2(20); -- number of days current year has in first week of year
   days_last_week_          VARCHAR2(20); -- number of days current year has in last week of year
   weeks_in_year_           NUMBER := 0; -- number of ISO weeks, either 52 or 53
   days_in_year_            NUMBER := 0; -- number of days, 365 or 366 if Leap Year
   num_days_feb_            NUMBER := 0; -- used to calculate days_in_year
   extra_weeks_             NUMBER := 0; -- used to track how many weeks in year
   counter_weeks_start_     NUMBER := 1;
   exists_                  NUMBER;      -- dummy variable for cursor select into
   --

   CURSOR check_period_exists(
             plan_year_no_          NUMBER,
             plan_year_period_no_   NUMBER,
             plan_period_unit_      NUMBER) IS
      SELECT 1
      FROM Plan_Period_Unit_Def_Tab
      WHERE plan_year_no        = plan_year_no_
      AND   plan_year_period_no = plan_year_period_no_
      AND   plan_period_unit    = plan_period_unit_;

BEGIN

--
-- Insert four years worth of default day units (i.e., for the current year, the
-- previous year and the next two years), into PLAN_PERIOD_UNIT_DEF_TAB; for weeks,
-- months and quarters insert seven years worth (i.e., for the current year, the
-- previous year and the next five years), into PLAN_PERIOD_UNIT_DEF_TAB;
--

   start_date_         := ADD_MONTHS(TRUNC(TO_DATE(TO_CHAR(SYSDATE, 'DD-MM-YYYY'), 'DD-MM-YYYY'), 'YYYY'), -12);
   start_date_day_     := start_date_;
   start_year_         := to_number(TO_CHAR(start_date_, 'YYYY'));

   FOR year_ IN start_year_..start_year_ + 6 LOOP

      start_date_week_    := start_date_day_;
      start_date_month_   := start_date_day_;
      start_date_quarter_ := start_date_day_;

      -- we use the number of days in FEBRUARY to determine number of days in year
      -- we do this regardless of the inserts since we need the days_in_year_ for
      -- inserting weeks and we need to keep incrementing the start_date_day_ even
      -- if we do not insert a day plan period unit record...

      SELECT TO_NUMBER(TO_CHAR(LAST_DAY(ADD_MONTHS(start_date_day_, 1)), 'dd'))
         INTO num_days_feb_
         FROM DUAL;

      IF num_days_feb_ = 29 THEN
         -- Leap Year
         days_in_year_ := 366;
      ELSE
         days_in_year_ := 365;
      END IF;

      FOR day_counter_ IN 1..days_in_year_ LOOP -- create a day plan period unit for each day of the year

         SELECT Plan_Period_Def_No.NEXTVAL
            INTO   plan_period_unit_def_no_
            FROM   DUAL;

         IF year_ < start_year_ + 4 THEN

            -- create day plan period units for 4 years only

            OPEN check_period_exists(
                plan_year_no_          => year_,
                plan_year_period_no_   => day_counter_,
                plan_period_unit_      => '1');
            FETCH check_period_exists INTO exists_;
            IF check_period_exists%NOTFOUND THEN
               INSERT INTO plan_period_unit_def_tab (
                  plan_period_unit_def_no,
                  plan_year_no,
                  plan_year_period_no,
                  begin_date,
                  end_date,
                  description,
                  plan_period_unit,
                  rowversion)
               VALUES (
                  plan_period_unit_def_no_,
                  year_,
                  day_counter_,
                  TRUNC(start_date_day_),
                  TRUNC(start_date_day_),
                  NULL,
                  '1',
                  sysdate);
            END IF;
            CLOSE check_period_exists;
         END IF; -- end day plan period unit creation for current year

         start_date_day_ := start_date_day_ + 1;

      END LOOP;


      -- create ISO week plan period units for the current year

      -- determine the number of weeks for the current year; where the first and
      -- last weeks of a year overlap the preceeding and successive year, the week
      -- is said to belong to the year that has 4 or more days of the week;

      -- in the selects that populate first_day_first_week_ and last_day_last_week_
      -- we decrement the start_date_day_ to bring us back to the previous year
      -- to create weeks for that year

      SELECT TO_CHAR(trunc(start_date_week_, 'YYYY'), 'DAY', 'NLS_DATE_LANGUAGE = AMERICAN')
         INTO first_day_first_week_
         FROM DUAL;

      SELECT TO_CHAR(LAST_DAY(ADD_MONTHS(TRUNC(start_date_week_, 'YYYY'), 11)), 'DAY', 'NLS_DATE_LANGUAGE = AMERICAN')
         INTO last_day_last_week_
         FROM DUAL;

      SELECT DECODE(rtrim(ltrim(first_day_first_week_)), 'MONDAY', '7', 'TUESDAY', '6', 'WEDNESDAY', '5', 'THURSDAY', '4', 'FRIDAY', '3', 'SATURDAY', '2', 'SUNDAY', '1') INTO days_first_week_ FROM DUAL;

      SELECT DECODE(rtrim(ltrim(last_day_last_week_)), 'MONDAY', '1', 'TUESDAY', '2', 'WEDNESDAY', '3', 'THURSDAY', '4', 'FRIDAY', '5', 'SATURDAY', '6', 'SUNDAY', '7') INTO days_last_week_ FROM DUAL;


      -- for the first and last week of a year, the week 'belongs'
      -- to the year that has > 4 days

      IF TO_NUMBER(days_first_week_) > 3 THEN
         -- the first week is counted as a week for that year
         extra_weeks_ := extra_weeks_ + 1;
      END IF;

      IF TO_NUMBER(days_last_week_ ) > 3 THEN
         -- the last week is counted as a week for that year
         extra_weeks_ := extra_weeks_ + 1;
      END IF;


      weeks_in_year_ := (days_in_year_ - days_first_week_ - days_last_week_)/7 + extra_weeks_;


      IF TO_NUMBER(days_first_week_) > 3 THEN
         -- the first week is counted as a week for that year
         -- reset start_date_week_ back to the beginning of the week
         start_date_week_ := start_date_week_ - (7 - days_first_week_);
         SELECT Plan_Period_Def_No.NEXTVAL
            INTO   plan_period_unit_def_no_
            FROM   DUAL;

         OPEN check_period_exists(
             plan_year_no_          => year_,
             plan_year_period_no_   => 1,
             plan_period_unit_      => '2');
         FETCH check_period_exists INTO exists_;
         IF check_period_exists%NOTFOUND THEN
            INSERT INTO plan_period_unit_def_tab (
               plan_period_unit_def_no,
               plan_year_no,
               plan_year_period_no,
               begin_date,
               end_date,
               description,
               plan_period_unit,
               rowversion)
            VALUES (
               plan_period_unit_def_no_,
               year_,
               1,
               TRUNC(start_date_week_),
               TRUNC(start_date_week_ + 6),
               NULL,
               '2',
               sysdate);

               counter_weeks_start_ := counter_weeks_start_ + 1;
         END IF;
         CLOSE check_period_exists;

         start_date_week_ := start_date_week_ + 7;

      ELSE
         start_date_week_ := start_date_week_ + days_first_week_;
      END IF;


      IF TO_NUMBER(days_last_week_ ) > 3 THEN
         -- the last week is counted as a week for that year
         -- reset start_date_week_ back to the beginning of the last week
         start_date_last_week_ := LAST_DAY(ADD_MONTHS(TRUNC(start_date_week_ - 1, 'YYYY'), 11)) - (days_last_week_ - 1);
         SELECT Plan_Period_Def_No.NEXTVAL
            INTO   plan_period_unit_def_no_
            FROM   DUAL;

         OPEN check_period_exists(
             plan_year_no_          => year_,
             plan_year_period_no_   => weeks_in_year_,
             plan_period_unit_      => '2');
         FETCH check_period_exists INTO exists_;
         IF check_period_exists%NOTFOUND THEN
            INSERT INTO plan_period_unit_def_tab (
               plan_period_unit_def_no,
               plan_year_no,
               plan_year_period_no,
               begin_date,
               end_date,
               description,
               plan_period_unit,
               rowversion)
            VALUES (
               plan_period_unit_def_no_,
               year_,
               weeks_in_year_,
               TRUNC(start_date_last_week_),
               TRUNC(start_date_last_week_ + 6),
               NULL,
               '2',
               sysdate);
         END IF;
         CLOSE check_period_exists;

         weeks_in_year_ := weeks_in_year_ - 1;
      END IF;

      FOR week_counter_ IN counter_weeks_start_..weeks_in_year_ LOOP -- create week plan period units

         SELECT Plan_Period_Def_No.NEXTVAL
            INTO   plan_period_unit_def_no_
            FROM   DUAL;

         OPEN check_period_exists(
             plan_year_no_          => year_,
             plan_year_period_no_   => week_counter_,
             plan_period_unit_      => '2');
         FETCH check_period_exists INTO exists_;
         IF check_period_exists%NOTFOUND THEN
            INSERT INTO plan_period_unit_def_tab (
               plan_period_unit_def_no,
               plan_year_no,
               plan_year_period_no,
               begin_date,
               end_date,
               description,
               plan_period_unit,
               rowversion)
            VALUES (
               plan_period_unit_def_no_,
               year_,
               week_counter_,
               TRUNC(start_date_week_),
               TRUNC(start_date_week_ + 6),
               NULL,
               '2',
               sysdate);
         END IF;
         CLOSE check_period_exists;

         start_date_week_ := start_date_week_ + 7;

      END LOOP; -- end create week plan period units


      -- create month plan period units current year

      FOR month_counter_ IN 1..12 LOOP -- create month plan period units

         SELECT Plan_Period_Def_No.NEXTVAL
            INTO   plan_period_unit_def_no_
            FROM   DUAL;

         OPEN check_period_exists(
             plan_year_no_          => year_,
             plan_year_period_no_   => month_counter_,
             plan_period_unit_      => '3');
         FETCH check_period_exists INTO exists_;
         IF check_period_exists%NOTFOUND THEN
            INSERT INTO plan_period_unit_def_tab (
               plan_period_unit_def_no,
               plan_year_no,
               plan_year_period_no,
               begin_date,
               end_date,
               description,
               plan_period_unit,
               rowversion)
            VALUES (
               plan_period_unit_def_no_,
               year_,
               month_counter_,
               TRUNC(start_date_month_),
               TRUNC(LAST_DAY(start_date_month_)),
               NULL,
               '3',
               sysdate);
         END IF;
         CLOSE check_period_exists;

         start_date_month_ := ADD_MONTHS(start_date_month_, 1);

      END LOOP; -- end create month plan period unit

      -- create quarter plan period units for the current year

      FOR quarter_counter_ IN 1..4 LOOP -- create quarter plan period units

         SELECT Plan_Period_Def_No.NEXTVAL
            INTO   plan_period_unit_def_no_
            FROM   DUAL;

         OPEN check_period_exists(
             plan_year_no_          => year_,
             plan_year_period_no_   => quarter_counter_,
             plan_period_unit_      => '4');
         FETCH check_period_exists INTO exists_;
         IF check_period_exists%NOTFOUND THEN
            INSERT INTO plan_period_unit_def_tab (
               plan_period_unit_def_no,
               plan_year_no,
               plan_year_period_no,
               begin_date,
               end_date,
               description,
               plan_period_unit,
               rowversion)
            VALUES (
               plan_period_unit_def_no_,
               year_,
               quarter_counter_,
               TRUNC(start_date_quarter_),
               TRUNC(LAST_DAY(ADD_MONTHS(start_date_quarter_, 2))),
               NULL,
               '4',
               sysdate);
         END IF;
         CLOSE check_period_exists;

         start_date_quarter_ := ADD_MONTHS(start_date_quarter_, 3);

      END LOOP; -- end create quarter plan period unit

      -- now reset all the year-specific counters
      days_in_year_        := 0;
      num_days_feb_        := 0;
      weeks_in_year_       := 0;
      extra_weeks_         := 0;
      counter_weeks_start_ := 1;

   END LOOP;

END;
/


